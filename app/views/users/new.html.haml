= form_for @new_user, url: users_path, method: :post do |f|
	Name:
	= f.text_field :name
	%br
	About:
	= f.text_area :about
	%br
	Email: (this will be used for log in)
	= f.email_field :contact
	%br
	Password
	= f.password_field :password
	%br
	Password Confirmation
	= f.password_field :password_confirmation
	%br

	=f.file_field :user_photo
	=f.submit "Sign up"